      <SubComponents>
        <Component class="javax.swing.JLabel" name="jLabel1">
          <Properties>
            <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
              <Font name="Tahoma" size="14" style="1"/>
            </Property>
            <Property name="text" type="java.lang.String" value="INFORMACION PREVIA PARA EL LLENADO DE LA TARJETA"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel2">
          <Properties>
            <Property name="text" type="java.lang.String" value="Aclaramos ciertos puntos que le ayudar&#xe1;n en el correcto llenado de la tarjeta:"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel3">
          <Properties>
            <Property name="text" type="java.lang.String" value="Se incluir&#xe1;n en la tarjeta solamente a aquellas personas que habitan en la misma vivienda y comen de la misma olla"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel4">
          <Properties>
            <Property name="text" type="java.lang.String" value="Cuando una vivenda es habitada por mas de una familia y existe independencia econ&#xf3;mica entre ellas, se llenar&#xe1;"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel5">
          <Properties>
            <Property name="text" type="java.lang.String" value="una tarjeta familiar por cada una. Si en la segunda familia no hay ni&#xf1;o afiliado a la Entidad Afiliada, no se llenar&#xe1;"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel6">
          <Properties>
            <Property name="text" type="java.lang.String" value="tarjeta, solo se anota en observaciones que se convive con esta familia y se menciiona el n&#xfa;mero de miembros"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel7">
          <Properties>
            <Property name="text" type="java.lang.String" value="Cuando en una vivienda vive mas de una familia pero en cada una de ellas hay ni&#xf1;os afiliados, se llenar&#xe1;n las tarjetas"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel8">
          <Properties>
            <Property name="text" type="java.lang.String" value="por separado, anotando en observaciones que ambas familias viven en el mimo lugar"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel9">
          <Properties>
            <Property name="text" type="java.lang.String" value="Cuando un miembro de la familia no est&#xe1; en ese momento, pero su ausencia ser&#xe1; temporal (m&#xe1;ximo tres mese) y"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel10">
          <Properties>
            <Property name="text" type="java.lang.String" value="regresar&#xe1; a seguir habitando en la vivienda, se lo deber&#xe1; incluir en la tarjeta."/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel11">
          <Properties>
            <Property name="text" type="java.lang.String" value="En el caso de los j&#xf3;venes que ingresan al Cuartel para hacer su Servicio Militar, se los deber&#xe1; incluir en la tarjeta"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel12">
          <Properties>
            <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
              <Font name="Tahoma" size="11" style="1"/>
            </Property>
            <Property name="text" type="java.lang.String" value="VACUNAS NI&#xd1;0S/MADRES"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel13">
          <Properties>
            <Property name="text" type="java.lang.String" value="Respecto a vacunas ni&#xf1;os/madres, solo deber&#xe1; anotarse para los ni&#xf1;os/as entre los 12 y 23.9 meses de edad."/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel14">
          <Properties>
            <Property name="text" type="java.lang.String" value="Anotar una &quot;X&quot; en SI, siempre y cuando se tenga completo el esquema de vacunaci&#xf3;n, es decir:"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel15">
          <Properties>
            <Property name="text" type="java.lang.String" value="Dosis &#xfa;nica de tuberculosais (BCG) al nacer, o lo mas pronto posible despu&#xe9;s de nacer"/>
          </Properties>
        </Component>
        <Component class="javax.swing.JLabel" name="jLabel16">
          <Properties>
            <Property name="text" type="java.lang.String" value="3 dosis de vacuna pentavalente (difteria, tosferina, t&#xe9;tano, neumon&#xed;a y hepatitis)"/>
          </Properties>
