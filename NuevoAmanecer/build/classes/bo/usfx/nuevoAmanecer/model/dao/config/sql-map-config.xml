	<sqlMap resource="bo/usfx/nuevoAmanecer/model/dao/ibatis/maps/Integrantes.xml" />
	<sqlMap resource="bo/usfx/nuevoAmanecer/model/dao/ibatis/maps/Padrinos.xml" />
	<sqlMap resource="bo/usfx/nuevoAmanecer/model/dao/ibatis/maps/Proyecto.xml" />
	<sqlMap resource="bo/usfx/nuevoAmanecer/model/dao/ibatis/maps/Encargados.xml" />
