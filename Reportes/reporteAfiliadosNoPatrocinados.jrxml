	<queryString>
		<![CDATA[SELECT
  familia.ci,
  UPPER(TRIM(familia.nombres) || ' ' || TRIM(familia.apellido_paterno) || ' ' || TRIM(familia.apellido_materno)) AS Afiliado,
  CASE WHEN familia.sexo = 'F' THEN 'FEMENINO' ELSE 'MASCULINO' END AS sexo,
  familia.fecha_nacimiento,
  familia.codigo_persona,
  afiliado.numero_caso,
  afiliado.numero_nino

FROM
  public.familia INNER JOIN public.afiliado
  ON afiliado.codigo_persona = familia.codigo_persona
WHERE afiliado.numero_caso NOT IN (SELECT CASO.numero_caso FROM CASO)
order by 2]]>
	</queryString>
