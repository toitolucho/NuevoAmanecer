	<queryString>
		<![CDATA[SELECT
  familia.ci,
  familia.nombres,
  familia.apellido_paterno,
  familia.apellido_materno,
  familia.sexo,
  familia.fecha_nacimiento,
  ocupaciones.nombre_ocupacion,
  actividad_educativa.nombre_actividad_educativa,
  proyecto.nombre_proyecto,
  proyecto.departamento,
  tarjeta.comunidad,
  tarjeta.barrio,
  tarjeta.numero_familia,
  tarjeta.observaciones,
  tarjeta.numero_proyecto
FROM
  public.familia
  INNER JOIN public.tarjeta
  ON tarjeta.numero_familia = familia.numero_familia
  INNER JOIN public.proyecto
  ON tarjeta.numero_proyecto = proyecto.numero_proyecto
  LEFT JOIN public.ocupaciones
  ON ocupaciones.codigo_ocupacion = familia.codigo_ocupacion
  LEFT JOIN public.actividad_educativa
  ON actividad_educativa.codigo_actividad_educactiva = familia.codigo_actividad_educactiva]]>
