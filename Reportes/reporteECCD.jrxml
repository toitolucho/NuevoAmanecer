  eccd.peso,
  eccd.talla,
  eccd.peso_talla,
  eccd.sven,
  eccd.numero_caso,
  UPPER(familia.nombres || ' ' || familia.apellido_paterno || ' ' || familia.apellido_materno) AS afiliado,
  familia.fecha_nacimiento
FROM public.familia
INNER JOIN public.afiliado
