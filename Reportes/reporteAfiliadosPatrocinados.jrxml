	<queryString>
		<![CDATA[SELECT
  familia.ci,
  familia.apellido_paterno || ' ' || familia.apellido_materno || ' ' || familia.nombres as nombre_completo,
  CASE WHEN familia.sexo = 'F' THEN 'FEMENINO' ELSE 'MASCULINO' END AS Sexo,
  familia.fecha_nacimiento,
  afiliado.numero_caso,
  afiliado.numero_nino
FROM
  public.familia,
  public.afiliado,
  public.caso
WHERE
  afiliado.codigo_persona = familia.codigo_persona AND
  afiliado.numero_caso = caso.numero_caso
order by 2;]]>
