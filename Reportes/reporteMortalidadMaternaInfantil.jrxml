	<queryString>
		<![CDATA[SELECT 'TOTAL MUERTES 0 - 5 AÑOS:' AS NOMBRE , COUNT(*) AS VALOR_ninos_muertos_0_5_I47
	FROM FAMILIA
	WHERE codigo_causa_muerte IS NOT NULL
	AND obtenerEdad(fecha_nacimiento::date, $P{Fecha_Gestion}::date) <= 5
	AND ObtenerNroProyectoDesdeFamilia(numero_familia) = $P{NumeroProyecto}
UNION ALL
SELECT 'TASA DE MORTALIDAD INFANTIL:', ( SELECT COUNT(*) AS ninos_muertes_0_E43
	FROM FAMILIA
	WHERE codigo_causa_muerte IS NOT NULL
	AND obtenerEdad(fecha_nacimiento::date, $P{Fecha_Gestion}::date) = 0
	AND ObtenerNroProyectoDesdeFamilia(numero_familia) = $P{NumeroProyecto}
	) * 100 /
	(
	SELECT COUNT(*) AS ninos_nacidos_vivos_0_I19
	FROM FAMILIA
	WHERE obtenerEdad(fecha_nacimiento::date, $P{Fecha_Gestion}::date) = 0
	AND ObtenerNroProyectoDesdeFamilia(numero_familia) = $P{NumeroProyecto}
	) AS TASA_DE_MORTALIDAD_INFANTIL
UNION ALL
SELECT 'TASA DE MORTALIDAD INFANTIL MENORES DE 5 AÑOS:',
	(
	SELECT COUNT(*) AS VALOR_ninos_muertos_0_5_I47
	FROM FAMILIA
	WHERE codigo_causa_muerte IS NOT NULL
	AND obtenerEdad(fecha_nacimiento::date, $P{Fecha_Gestion}::date) <= 5
	AND ObtenerNroProyectoDesdeFamilia(numero_familia) = $P{NumeroProyecto}
	) * 100 /
	(
	SELECT COUNT(*) AS ninos_nacidos_vivos_0_I19
	FROM FAMILIA
	WHERE obtenerEdad(fecha_nacimiento::date, $P{Fecha_Gestion}::date) = 0
	AND ObtenerNroProyectoDesdeFamilia(numero_familia) = $P{NumeroProyecto}
	)
UNION ALL
SELECT 'TASA DE MORTALIDAD MATERNA:',
	(
	SELECT COUNT(*) AS muerte_maternal_M35
	FROM FAMILIA
	WHERE codigo_causa_muerte = 'R'
	AND ObtenerNroProyectoDesdeFamilia(numero_familia) = $P{NumeroProyecto}
	) * 100 /
	(
	SELECT COUNT(*) AS ninos_nacidos_vivos_0_I19
	FROM FAMILIA
	WHERE obtenerEdad(fecha_nacimiento::date, $P{Fecha_Gestion}::date) = 0
	AND ObtenerNroProyectoDesdeFamilia(numero_familia) = $P{NumeroProyecto}
	)]]>
